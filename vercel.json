{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/zh-TW",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/zh-TW"
    }
  ],
  "trailingSlash": false
}
