{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "/zh-TW/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/zh-TW/:path*",
      "destination": "/zh-TW/:path*"
    },
    {
      "source": "/en-US/:path*",
      "destination": "/en-US/:path*"
    }
  ],
  "trailingSlash": false
}
